@model IEnumerable<TravalApp1.Models.TouristSpot>

@{
    ViewData["Title"] = "Search Results";
}

<h1>Search Results</h1>

@if (Model != null && Model.Any())
{
    <ul>
        @foreach (var spot in Model)
        {
            <li>
                <h3>@spot.Name</h3>
                <p>@spot.Address</p>
                <img src="@spot.ImageUrl" alt="@spot.Name" />
            </li>
        }
    </ul>
}
else
{
    <p>No results found.</p>
}
